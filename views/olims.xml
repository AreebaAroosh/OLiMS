<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    	
    	 
        <!-- top level menu: no parent -->
        <menuitem id="main_olims_menu" name="OLiMS"/>
        <!-- A first level in the left side menu is needed
             before using action= attribute -->
        <menuitem id="olims_menu" name="OLiMS" parent="main_olims_menu"/>
        <!-- the following menuitem should appear *after*
             its parent olims_menu and *after* its
             action course_list_action -->
        

       <!--Client form view -->
        <record model="ir.ui.view" id="olims_client_form_view">
            <field name="name">client.form</field>
            <field name="model">olims.client</field>
            <field name="arch" type="xml">
                <form string="Client Form">
                     <label for="Name" string="Name"/>
                	<field name="Name"></field>
                	<label for="TaxNumber" string="Tax Number"/>
                    <field name="TaxNumber"></field>
                    <label for="Phone" string="Phone"/>
                	<field name="Phone"></field>
                    <label for="EmailAddress" string="Email Address"/>
                	<field name="EmailAddress"></field>
                    <label for="AccountType" string="Account Type"/>
                    <field name="AccountType"></field>
                    <label for="AccountName" string="Account Name"/>
                    <field name="AccountName"></field>
                    <label for="AccountNumber" string="Account Number"/>
                    <field name="AccountNumber"></field>
                    <label for="BankName" string="Bank Name"/>
                    <field name="BankName"></field>
                    <label for="BankBranch" string="Bank Branch"/>
                    <field name="BankBranch"></field>
                    <label for="ClientID" string="Client ID"/>
                    <field name="ClientID"></field>
                    <label for="BulkDiscount" string="Bulk Discount"/>
                    <field name="BulkDiscount"></field>
                    <label for="MemberDiscountApplies" string="Member Discount Applies"/>
                    <field name="MemberDiscountApplies"></field>
                    <label for="DefaultCategories" string="Default Categories"/>
                    <field name="DefaultCategories"></field>
                    <label for="RestrictedCategories" string="Restricted Categories"/>
                    <field name="RestrictedCategories"></field>
                    <label for="DefaultARSpecs" string="Default ARSpecs"/>
                    <field name="DefaultARSpecs"></field>
                    <label for="DefaultDecimalMark" string="Default DecimalMark"/>
                    <field name="DefaultDecimalMark"></field>
                    <label for="DecimalMark" string="Decimal Mark"/>
                    <field name="DecimalMark"></field>
                    </form>
            </field>
        </record>

        <record model="ir.ui.view" id="olims_client_tree_view">
            <field name="name">client.tree</field>
            <field name="model">olims.client</field>
            <field name="arch" type="xml">
                <tree string="CLient">
                	<field name="Name"></field>
                	<field name="TaxNumber"></field>
                	<field name="Phone"></field>
                	<field name="EmailAddress"></field>
                    <field name="AccountType"></field>
                    <field name="AccountName"></field>
                    <field name="AccountNumber"></field>
                    <field name="BankName"></field>
                    <field name="BankBranch"></field>
                    <field name="ClientID"></field>
                    <field name="BulkDiscount"></field>
                    <field name="MemberDiscountApplies"></field>
                    <field name="DefaultCategories"></field>
                    <field name="RestrictedCategories"></field>
                    <field name="DefaultARSpecs"></field>
                    <field name="DefaultDecimalMark"></field>
                    <field name="DecimalMark"></field>
                </tree>
            </field>
        </record>

		<record model="ir.actions.act_window" id="client_list_action">
            <field name="name">Client</field>
            <field name="res_model">olims.client</field>
            <field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
       </record>

        <menuitem id="client_menu" name="Client" parent="olims_menu"
                  action="client_list_action"/>

